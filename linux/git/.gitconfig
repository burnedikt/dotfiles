[user]
	name = Benedikt Reiser
	email = benedikt.reiser@gmail.com
[core]
	editor = /usr/bin/vim
	trustctime = false
	autocrlf = false
[push]
	default = simple
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[commit]
	# https://help.github.com/articles/signing-commits-using-gpg/
	gpgsign = false
